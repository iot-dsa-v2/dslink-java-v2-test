{
  "name": "dslink-java-v2-test",
  "version": "1.0.2",
  "description": "A Java DSLink for unit/integration testing the SDK and brokers",
  "main": "bin/dslink-java-v2-test",
  "configs": {
    "broker": {
      "type": "url",
      "value" : null
    },
    "main-node": {
      "type": "string",
      "value": "org.iot.dsa.dslink.test.MainNode"
    },
    "log": {
      "desc": "all, trace, debug, fine, warn, info, error, admin, fatal, none",
      "type": "enum",
      "value": "info"
    },
    "token": {
      "desc": "Authentication token for the broker.",
      "type": "string",
      "value": null
    }
  }
}
